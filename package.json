{
  "name": "unicode2latex",
  "version": "4.0.38",
  "keywords": [
    "TeX",
    "LaTeX",
    "unicode"
  ],
  "description": "Mapping to convert unicode characters from and to LaTeX commands",
  "main": "index.js",
  "scripts": {
    "test": "npm start && jest",
    "start": "npm run build && npm run docs && make",
    "build": "run-p --aggregate-output lint tsc docs",
    "lint": "eslint .",
    "tsc": "tsc",
    "docs": "typedoc --gitRevision --out docs --readme README.md index.ts",
    "preversion": "npm start",
    "postversion": "git push --tags && git push"
  },
  "author": "Emiliano Heyns",
  "license": "ISC",
  "files": [
    "package.json",
    "index.js",
    "index.d.ts",
    "tables/biblatex.json",
    "tables/bibtex.json",
    "tables/minimal.json",
    "tables/latex2unicode.json",
    "tables/combining.json"
  ],
  "devDependencies": {
    "@types/node": "^20.10.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/unicode2latex.git"
  },
  "homepage": "https://github.com/retorquere/unicode2latex.git"
}
